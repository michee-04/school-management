{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "core-app": {
      "type": "library",
      "root": "libs/core-app",
      "entryFile": "index",
      "sourceRoot": "libs/core-app/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core-app/tsconfig.lib.json"
      }
    },
    "prof": {
      "type": "library",
      "root": "libs/prof",
      "entryFile": "index",
      "sourceRoot": "libs/prof/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prof/tsconfig.lib.json"
      }
    },
    "notification": {
      "type": "library",
      "root": "libs/notification",
      "entryFile": "index",
      "sourceRoot": "libs/notification/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notification/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}